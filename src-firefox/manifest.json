{
  "name": "Launchpad",
  "version": "2.2.1",
  "manifest_version": 2,
  "description": "Display a grid of bookmarks with customisable icons on your new tab page",
  "homepage_url": "http://github.com/zarino/launchpad",
  "icons": {
    "128": "icons/icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "chrome_settings_overrides": {
    "homepage": "index.html"
  },
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "applications": {
    "gecko": {
      "id": "launchpad@zarino.co.uk"
    }
  }
}